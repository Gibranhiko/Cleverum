{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "outDir": "dist",
      "rootDir": ".",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true
    },
    "exclude": ["node_modules"]
  }
  